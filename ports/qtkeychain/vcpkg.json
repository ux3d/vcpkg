{
  "name": "qtkeychain",
  "version": "0.13.2",
  "port-version": 2,
  "description": "(Unaffiliated with Qt) Platform-independent Qt5 API for storing passwords securely",
  "homepage": "https://github.com/frankosterfeld/qtkeychain",
  "license": "BSD-3-Clause",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "libsecret",
    "translations"
  ],
  "features": {
    "libsecret": {
      "description": "Build qtkeychain with libsecret support",
      "supports": "!(windows | uwp | osx)",
      "dependencies": [
        "libsecret"
      ]
    },
    "translations": {
      "description": "Build qtkeychain translations",
      "dependencies": [
        "qt5-tools"
      ]
    }
  }
}
